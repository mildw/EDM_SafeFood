result-view {
	match{
		InfoResult(this) {
			from-output: Info
		}
	}
	
	render {
		if(size(this) > 1) {
			list-of (this) {
				where-each (infoResult) {
					layout-macro (info_list_macro) {
						param (infoResult) {
							expression (infoResult)
						}
					}
				}
			}
		}
		else-if(size(this) == 1) {
			layout-match (this) {
				mode(Details)
			}
		}
	}
}