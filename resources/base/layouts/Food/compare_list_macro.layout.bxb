layout-macro-def(compare_list_macro) {
	params {
		param (compareResult) {
			type (CompareResult)
			min (Required)
			max (One)
		}
	}

	content {

		image-card{

			aspect-ratio (3:4)
			image-url ("#{value(compareResult)}")

			title-area{
				slot1{
					text{
						value("")
						style(Title_S)
					}
				}

				slot2{
					paragraph{
						value("")
						style(Detail_L)
					}
				}
			}
		}
	}
}