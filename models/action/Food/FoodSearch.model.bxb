action (FoodSearch) {
  description (신라면 식품 정보 알려줘)
  type(Search)
  collect {
    input (foodName) {
      type (FoodName)
      min (Optional) max (One)
    }
  }
  output (FoodOne) {
		throws {
			error (Error) {
				on-catch {
					halt {
						dialog {
							template ("공공데이터 서버 또는 외부 서버 통신에 문제가 발생했습니다.	잠시후 다시 시도해 주세요")
						}
					}
				}
			}
		}
	}
}
